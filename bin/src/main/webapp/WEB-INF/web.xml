<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <!-- by default the program first searches for the welcome file list
  and if not found then goes for index.html or index.jsp
  so since we dont want to start with index.jsp we will define it here -->
  
  <welcome-file-list>
  	
  	<welcome-file>home.jsp</welcome-file>
  	
  </welcome-file-list>
</web-app>
